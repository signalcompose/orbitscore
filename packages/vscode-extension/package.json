{
  "name": "@orbitscore/vscode-extension",
  "displayName": "OrbitScore",
  "publisher": "local",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:orbitscore.start",
    "onCommand:orbitscore.runSelection",
    "onCommand:orbitscore.stop",
    "onCommand:orbitscore.transport"
  ],
  "contributes": {
    "commands": [
      {
        "command": "orbitscore.start",
        "title": "OrbitScore: Start Engine"
      },
      {
        "command": "orbitscore.runSelection",
        "title": "OrbitScore: Run Selection"
      },
      {
        "command": "orbitscore.stop",
        "title": "OrbitScore: Stop Engine"
      },
      {
        "command": "orbitscore.transport",
        "title": "OrbitScore: Transport Panel"
      }
    ],
    "languages": [
      {
        "id": "orbitscore",
        "extensions": [
          ".osc"
        ]
      }
    ],
    "keybindings": [
      {
        "command": "orbitscore.runSelection",
        "key": "cmd+enter",
        "when": "editorTextFocus && editorLangId == orbitscore"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/vscode": "^1.90.0"
  }
}
